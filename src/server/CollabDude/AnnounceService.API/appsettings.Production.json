{
  "ConnectionStrings": {
    "DefaultConnection": "${DATABASE_CONNECTION_STRING}"
  },
  "JwtSettings": {
    "SecretKey": "${JWT_SECRET_KEY}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "ExpiryMinutes": 30
  },
  "AllowedOrigins": [
    "${FRONTEND_URL}",
    "${ADMIN_PANEL_URL}"
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "AnnounceService": "Information"
    }
  },
  "ApiSettings": {
    "MaxPageSize": 50,
    "DefaultPageSize": 10
  }
}